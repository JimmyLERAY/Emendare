{
  "extends": ["tslint:recommended", "tslint-react-hooks"],
  "rules": {
    "semicolon": false,
    "quotemark": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "max-line-length": false,
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "variable-name": false,
    "react-hooks-nesting": "error"
  }
}
